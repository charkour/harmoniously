  "dependencies": {
    "canvas-confetti": "1.3.3",
    "harmoniously": "0.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@harmoniously/shared": "0.0.5",
    "@size-limit/preset-small-lib": "4.10.1",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-info": "5.3.21",
